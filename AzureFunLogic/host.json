{
    "version": "2.0",
    "functions": ["SimpleHttpFunction"],
    "functionTimeout": "00:10:00",
    "logging": {
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Request"
            },
            "enableLiveMetricsFilters": true
        },
        "logLevel": {
            "Function": "Information",
            "Host": "Information",
            "Default": "Information"
        }
    }
}